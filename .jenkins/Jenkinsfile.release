@Library('pipeline-shared-library@master') _

pipeline {
  agent {
    label 'ec2-agent'
  }
  options {
    gitLabConnection(env.GITLAB_CONNECTION_NAME)
    skipDefaultCheckout(true)
    ansiColor('xterm')
  }
  stages {
    stage('Checkout') {
      steps {
        ciCheckout()
      }
    }
  }
}
