<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="tsaas-bridge-1.0.8-1" author="mbartecki">
        <addColumn tableName="hit">
            <column name="watchlist_name" type="VARCHAR(1024)">
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>

    <changeSet id="tsaas-bridge-1.0.8-2" author="ahaczewski">
        <addColumn tableName="hit">
            <column name="free_text_agent_request" type="TEXT"/>
            <column name="crossmatch_agent_request" type="TEXT"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>
